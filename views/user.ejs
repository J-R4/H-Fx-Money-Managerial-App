<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Page</title>
</head>
<body>
    <h2>User Page</h2>
    <div class="totalbalance">
        <p >Total Balance :</p>
        <% let total = 0 %>
        <% if(bankAccount){%>
           <% bankAccount.forEach(e => {%>
               <% total += e.balance%>
            <%})%>
            <p> <%= total%></p><br><br>
        <%}%>

        <table>
            <tr>
                <th>Bank Name</th>
                <th>Balance</th>
            </tr>
            for(let i =0; i < bankAccount.length; i++){
                <tr>
                    <td><%= banks.bank_name</td>
                    <td><%= banks[i].balance</td>
                </tr>
            }
        </table>




    </div>    
</body>
</html>